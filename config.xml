<?xml version="1.0" encoding="UTF-8"?>	<widget xmlns = "http://www.w3.org/ns/widgets"		xmlns:gap = "http://phonegap.com/ns/1.0"		id        = "app.SBI.hotfest"		versionCode="11"		version   = "0.3.6">		<name>HOTFEST</name>		<description>Vive la exclusividad de la información,HOTFEST la app de eventos más actualizada.		</description>		<author href="kol3.com" email="j.escudero@kol3.com">Johnny Escudero		</author>		<preference name="phonegap-version" value="3.6.3" />		<preference name="orientation" value="portrait" />		<preference name="fullscreen" value="false" />		<preference name="target-device" value="handset" />		<preference name="webviewbounce" value="true" />		<preference name="prerendered-icon" value="true" />		<preference name="stay-in-webview" value="true" />		<preference name="ios-statusbarstyle" value="black-transparent" />		<preference name="detect-data-types" value="true" />		<preference name="exit-on-suspend" value="false" />		<preference name="show-splash-screen-spinner" value="true" />		<preference name="auto-hide-splash-screen" value="true" />		<preference name="EnableViewportScale" value="true" />		<preference name="MediaPlaybackRequiresUserAction" value="false" />		<preference name="AllowInlineMediaPlayback" value="false" />		<preference name="BackupWebStorage" value="none" />		<preference name="TopActivityIndicator" value="gray" />		<preference name="KeyboardDisplayRequiresUserAction" value="false" />		<preference name="HideKeyboardFormAccessoryBar" value="false" />		<preference name="SuppressesIncrementalRendering" value="false" />		<preference name="android-minSdkVersion" value="16" />		<preference name="android-installLocation" value="internalOnly" />		<preference name="SplashScreenDelay" value="5000" />		<preference name="ErrorUrl" value="Error :("/>		<preference name="BackgroundColor" value="0x000000"/>		<preference name="KeepRunning" value="true"/>		<preference name="DisallowOverscroll" value="false"/>		<preference name="LoadingDialog" value="Cargando eventos"/> 		<preference name="LoadUrlTimeoutValue" value="1000" />		<preference name="disable-cursor" value="false" />		<gap:platform name="ios" />		<gap:platform name="android" />		<gap:platform name="winphone" />		<feature name="http://api.phonegap.com/1.0/media"/>		<feature name="http://api.phonegap.com/1.0/notification"/>		<feature name="http://api.phonegap.com/1.0/media"/>		<feature name="http://api.phonegap.com/1.0/network"/>    	<gap:plugin name="de.appplant.cordova.plugin.local-notification" version="0.7.5" />		<gap:plugin name="net.tunts.webintent" version="0.2.1" />		<gap:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />		<gap:plugin name="org.apache.cordova.splashscreen" version="0.3.4" />		<icon src="res/icon.png" />		<icon gap:density="ldpi" gap:platform="android" src="res/icons/android/ldpi.png" /> <!-- 36 pixels -->	    <icon gap:density="mdpi" gap:platform="android" src="res/icons/android/mdpi.png" /> <!-- 48 pixels -->	    <icon gap:density="hdpi" gap:platform="android" src="res/icons/android/hdpi.png" /> <!-- 72 pixels -->	    <icon gap:density="xhdpi" gap:platform="android" src="res/icons/android/xhdpi.png" /> <!-- 96 pixels -->	    <gap:splash gap:density="ldpi" gap:platform="android" src="res/screens/android/screen-ldpi-landscape.png" />	    <gap:splash gap:density="mdpi" gap:platform="android" src="res/screens/android/screen-mdpi-landscape.png" />	    <gap:splash gap:density="hdpi" gap:platform="android" src="res/screens/android/screen-hdpi-landscape.png" />	    <gap:splash gap:density="xhdpi" gap:platform="android" src="res/screens/android/screen-xhdpi-landscape.png" />		<access origin="*" />	</widget>